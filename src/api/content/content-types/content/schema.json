{
  "kind": "collectionType",
  "collectionName": "contents",
  "info": {
    "singularName": "content",
    "pluralName": "contents",
    "displayName": "Content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "jenis_content": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::jenis-content.jenis-content",
      "inversedBy": "contents"
    },
    "creators": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::creator.creator",
      "inversedBy": "contents"
    }
  }
}
